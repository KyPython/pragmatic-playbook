{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/server.js"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ],
  "env": {
    "HUBSPOT_API_KEY": "@hubspot_api_key",
    "HUBSPOT_PORTAL_ID": "@hubspot_portal_id"
  }
}

